<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="650.0" maxWidth="500.0" minHeight="650.0" minWidth="500.0" prefHeight="650.0" prefWidth="500.0" style="-fx-background-color: #4286f4" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gameOverController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="-16.0" layoutY="8.0" prefHeight="117.0" prefWidth="537.0" text="Game Over" textAlignment="CENTER">
         <font>
            <Font size="72.0" />
         </font>
      </Label>
      <Button layoutX="160.0" layoutY="303.0" mnemonicParsing="false" onMouseClicked="#mainMenu" text="Main Menu" textFill="#5f2bd0">
         <font>
            <Font name="Comic Sans MS" size="31.0" />
         </font>
      </Button>
      <Button layoutX="160.0" layoutY="382.0" mnemonicParsing="false" onMouseClicked="#newGame" prefHeight="65.0" prefWidth="197.0" text="Restart" textFill="#5f2bd0">
         <font>
            <Font name="Comic Sans MS" size="31.0" />
         </font>
      </Button>
      <Button layoutX="160.0" layoutY="462.0" mnemonicParsing="false" onMouseClicked="#exit" prefHeight="65.0" prefWidth="197.0" text="Quit" textFill="#5f2bd0">
         <font>
            <Font name="Comic Sans MS" size="31.0" />
         </font>
      </Button>
      <Label layoutX="195.0" layoutY="208.0" text="Score">
         <font>
            <Font size="41.0" />
         </font>
      </Label>
      <Label fx:id="score" layoutX="220.0" layoutY="163.0" text="990">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
